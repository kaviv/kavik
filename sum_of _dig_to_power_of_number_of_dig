#include <stdio.h>
int main(void) {
  int n,dig,count=0,rev=0,i,sum=0,temp;
  scanf("%d",&n);
  temp=n;
  while(temp!=0)
  {
    dig=temp%10;
    rev=rev*10+dig;
    temp=temp/10;
    count=count+1;
  }
  for(i=1;i<=count;i++)
  {
    dig=rev%10;
    temp=pow(dig,count);
    rev=rev/10;
    sum=sum+temp;
  }
    printf("%d",sum);
    return 0;
}
